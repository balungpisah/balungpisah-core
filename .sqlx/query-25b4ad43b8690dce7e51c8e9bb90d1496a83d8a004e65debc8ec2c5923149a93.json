{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                ta.id as attachment_id,\n                ta.thread_id,\n                ta.created_at as attachment_created_at,\n                f.id as file_id,\n                f.file_key,\n                f.original_filename,\n                f.content_type,\n                f.file_size\n            FROM thread_attachments ta\n            JOIN files f ON ta.file_id = f.id\n            WHERE ta.thread_id = $1 AND f.is_active = TRUE\n            ORDER BY ta.created_at ASC\n            ",
