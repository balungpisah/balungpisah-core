{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                rc.category_id,\n                c.name as category_name,\n                c.slug as category_slug,\n                rc.severity as \"severity: ReportSeverity\"\n            FROM report_categories rc\n            JOIN categories c ON c.id = rc.category_id\n            WHERE rc.report_id = $1\n            ORDER BY c.name\n            ",
