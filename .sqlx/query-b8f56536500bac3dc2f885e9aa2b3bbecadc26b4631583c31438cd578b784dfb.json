{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                r.id,\n                r.title,\n                r.description,\n                r.status as \"status: ReportStatus\",\n                r.timeline,\n                r.impact,\n                r.created_at\n            FROM reports r\n            WHERE r.created_at >= CURRENT_DATE - $1::int\n              AND r.status NOT IN ('draft', 'rejected')\n            ORDER BY r.created_at DESC\n            LIMIT $2\n            ",
